    "scripts": {
      "build": "cargo build --release",
      "format": "cargo fmt --all",
      "format:check": "cargo fmt --check --all",
      "lint": "cargo clippy --allow-dirty --allow-staged --all-targets --all-features --fix -- -D warnings",
